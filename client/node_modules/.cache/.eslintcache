iltroDietas, setFiltroDietas] = useState([]);\r\n  const [inputText, setInputText] = useState(\"\");\r\n\r\n  if (!dietas.length) {\r\n    dispatch(pedirDietas());\r\n  }\r\n\r\n  const handleSubmitOrigen = (event) => {\r\n    event.preventDefault();\r\n    let aux = [...origen];\r\n    dispatch(filterOrigen(aux));\r\n  };\r\n\r\n  const handleSubmitDietas = (event) => {\r\n    event.preventDefault();\r\n    let aux = [...filtroDietas];\r\n    console.log(aux);\r\n    dispatch(filterDiets(aux));\r\n  };\r\n\r\n  const handleDiet = (event) => {\r\n    if (!filtroDietas.includes(event.target.value))\r\n      setFiltroDietas([...filtroDietas, event.target.value]);\r\n    else {\r\n      setFiltroDietas(\r\n        filtroDietas.filter((element) => element !== event.target.value)\r\n      );\r\n    }\r\n  };\r\n\r\n  const handleOrigen = (event) => {\r\n    if (!origen.includes(event.target.value))\r\n      setOrigen([...origen, event.target.value]);\r\n    else {\r\n      setOrigen(origen.filter((element) => element !== event.target.value));\r\n    }\r\n  };\r\n\r\n  const handleChange = (event) => {\r\n    const valor = event.target.value;\r\n    setInputText(valor);\r\n  };\r\n\r\n  const handleSortAlf = (event) => {\r\n    event.preventDefault();\r\n    dispatch(sortAlfabeticamente(event.target.value));\r\n    setOrder(event.target.value);\r\n  };\r\n\r\n  const handleSortScore = (event) => {\r\n    event.preventDefault();\r\n    console.log(\"cambio el select Score\");\r\n    dispatch(sortScore(event.target.value));\r\n    setOrder(event.target.value);\r\n  };\r\n\r\n  const handleSubmitName = (event) => {\r\n    event.preventDefault();\r\n    try {\r\n      dispatch(searchName(inputText));\r\n    } catch (error) {\r\n      return error;\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className={styles.Contenedor}>\r\n      <form //! barra de busqueda\r\n        onSubmit={handleSubmitName}\r\n        style={{ display: \"flex\", width: \"100%\" }}\r\n      >\r\n        <input\r\n          type=\"text\"\r\n          placeholder=\"Bucar receta...\"\r\n          className={styles.InputBusqueda}\r\n          onChange={handleChange}\r\n          value={inputText}\r\n        />\r\n        <button type=\"submit\" className={styles.BotonBusqueda}>\r\n          &#x1F50E;&#xFE0E;\r\n        </button>\r\n      </form>\r\n      <br />\r\n      <div className={styles.Filtros}>\r\n        <h2>Filtrar por origen</h2>\r\n        <form //!filtro por origen\r\n          className={styles.Origen}\r\n          onSubmit={(event) => handleSubmitOrigen(event)}\r\n        >\r\n          <label className={styles.RadioInput}>\r\n            {\" \"}\r\n            <input\r\n              type=\"checkbox\"\r\n              name=\"FiltrarOrigen\"\r\n              value=\"online\"\r\n              onChange={(event) => handleOrigen(event)}\r\n            />\r\n            Online\r\n          </label>\r\n          <label className={styles.RadioInput}>\r\n            {\" \"}\r\n            <input\r\n              type=\"checkbox\"\r\n              name=\"FiltrarOrigen\"\r\n              value=\"propias\"\r\n              onChange={(event) => handleOrigen(event)}\r\n            />\r\n            Propias\r\n          </label>\r\n          <button className={styles.SubmitButton} type=\"submit\">\r\n            {\" \"}\r\n            Filtrar\r\n          </button>\r\n        </form>\r\n        <h2>Filtrar por dietas</h2>\r\n        <form onSubmit={(event) => handleSubmitDietas(event)}>\r\n          {\" \"}\r\n          {dietas.length ? (\r\n            dietas.map((element) => {\r\n              return (\r\n                <label className={styles.Dietas}>\r\n                  <input\r\n                    type=\"checkbox\"\r\n                    name=\"dietas\"\r\n                    value={element.name}\r\n                    onChange={handleDiet}\r\n                  />\r\n                  {element.name}\r\n                </label>\r\n              );\r\n            })\r\n          ) : (\r\n            <div>waiting</div>\r\n          )}\r\n          <button className={styles.SubmitButton} type=\"submit\">\r\n            {\" \"}\r\n            Filtrar\r\n          </button>\r\n        </form>\r\n        <h2>Ordenar</h2>\r\n        <div className={styles.Origen}>\r\n          <select //! select alfabetico\r\n            className={styles.Sorts}\r\n            onChange={(event) => handleSortAlf(event)}\r\n          >\r\n            <option disabled selected>\r\n              Orden alfabetico\r\n            </option>\r\n            <option value=\"a-z\"> A - Z</option>\r\n            <option value=\"z-a\"> Z - A</option>\r\n          </select>\r\n          <select //!select healthscore\r\n            className={styles.Sorts}\r\n            onChange={(event) => handleSortScore(event)}\r\n          >\r\n            <option disabled selected>\r\n              {\" \"}\r\n              Puntaje saludable{\" \"}\r\n            </option>\r\n            <option value=\"min-max\">Min - Max</option>\r\n            <option value=\"max-min\">Max - Min</option>\r\n          </select>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\HP\\Desktop\\PI-food-Vila\\client\\src\\components\\Recipe List\\RecipeList.jsx",["88","89"],"import { useEffect, useState } from \"react\";\r\nimport NavPages from \"../Navegador ed paginas/NavPages\";\r\nimport Recipe from \"../Recipe/Recipe\";\r\nimport styles from \"./RecipeList.module.css\";\r\nimport axios from \"axios\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport { getAllRecipes } from \"../../Redux/action\";\r\n\r\nexport default function RecipeList() {\r\n  // * peticion a axios donde recibo 100 dietas\r\n  // * pongo un estado con la current page que empiece en 1\r\n  // * si yo le doy al boton de ir adelante cambio el estado a etado +1 --- o si le doy a una pagina en particular configuro el estado al 